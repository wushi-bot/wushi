{
    "compilerOptions": {
      "baseUrl": "./",
      "target": "esnext",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": false,
      "strictNullChecks": false,
      "strictPropertyInitialization": false,
      "noImplicitAny": false,
      "typeRoots": [],
      "types": [],
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
    },
    "exclude": [
      "./node_modules/"
    ]
  }